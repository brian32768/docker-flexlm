    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "internalConsole"
        },
        {
            "name": "Python: Flexlm",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "PYTHONDONTWRITEBYTECODE": "1",
                "FLASK_APP": "start_app",
                "FLASK_ENV": "development",
            },
            "args": [
                "run",
                "--no-reload"
            ],
            "jinja": true,
            "console": "internalConsole"
        }
    ]
}